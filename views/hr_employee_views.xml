<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="modelo_ejemplo_view_tree" model="ir.ui.view">
        <field name="name">modelo.de.ejemplo.list</field>
        <field name="model">modelo.de.ejemplo</field>
        <field name="arch" type="xml">
            <tree string="Modelo de ejemplo" editable="bottom">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="modelo_ejemplo_view_form" model="ir.ui.view">
        <field name="name">modelo.de.ejemplo.form</field>
        <field name="model">modelo.de.ejemplo</field>
        <field name="arch" type="xml">
            <form string="Modelo de ejemplo">
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="martin_action" model="ir.actions.act_window">
        <field name="name">martinPrueba</field>
        <field name="res_model">modelo.de.ejemplo</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>